      "type": "string",
      "enum": [
        "PREFERRED_NAME",
        "GREETING",
        "CUSTOM_PLAYBACK_SPEEDS"
      ],
      "default": "PREFERRED_NAME"
    },
    "protobufgetCoursesResponse": {
      "type": "object",
      "properties": {
